{
    "name": "xstro",
    "version": "1.0.0",
    "description": "OpenSource WhatsApp Userbot",
    "type": "module",
    "main": "main.js",
    "scripts": {
        "start": "pm2 start . --attach --name xstro",
        "stop": "pm2 stop xstro",
        "build": "yarn run eslint . --fix && tsc && babel release --out-dir release",
        "semantic-release": "semantic-release"
    },
    "release": {
        "branches": [
            "master",
            {
                "name": "master",
                "prerelease": "alpha"
            },
            {
                "name": "master",
                "channel": "stable"
              }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/github"
        ]
    },
    "imports": {
        "#core": "./release/src/index.mjs"
    },
    "exports": {
        "import": "./release/src/index.mjs"
    },
    "files": [
        "release"
    ],
    "license": "MIT",
    "keywords": [
        "whatsapp-bot",
        "astrox11",
        "baileys"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AstroX11/xstro"
    },
    "bugs": {
        "url": "https://github.com/AstroX11/xstro/issues"
    },
    "homepage": "https://github.com/AstroX11/xstro#readme",
    "author": "AstroX11 <danielfx0012@gmail.com>",
    "devDependencies": {
        "@babel/core": "^7.26.4",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.1",
        "@semantic-release/release-notes-generator": "^14.0.3",
        "babel-cli": "^6.26.0",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "pino": "^9.6.0",
        "prettier": "^3.1.0",
        "semantic-release": "^24.2.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    },
    "dependencies": {
        "@hapi/boom": "^10.0.1",
        "baileys": "^6.7.13",
        "cheerio": "^1.0.0",
        "file-type": "20.2.0",
        "got": "^14.4.6",
        "jiti": "^2.4.2",
        "link-preview-js": "^3.0.14",
        "node-webpmux": "^3.2.0",
        "pm2": "^5.4.3",
        "qrcode-terminal": "^0.12.0",
        "sharp": "^0.33.5",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7",
        "youtubei.js": "^13.1.0"
    },
    "packageManager": "yarn@1.22.22"
}
