  "author": "AstroX11 <danielfx0012@gmail.com>",
  "devDependencies": {
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@astrox11/sqlite": "^1.8.5",
    "@hapi/boom": "^10.0.1",
    "baileys": "*",
    "dotenv": "^16.5.0",
    "file-type": "20.4.1",
    "got": "^14.4.7",
    "https-proxy-agent": "^7.0.6",
