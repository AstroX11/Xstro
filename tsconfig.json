{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"rootDir": ".",
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noEmit": true,
		"skipLibCheck": true,
		"noImplicitThis": true,
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true
	}
}
